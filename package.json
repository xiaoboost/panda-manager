    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "sharp": "^0.20.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/fs-extra": "^5.0.4",
