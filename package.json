{
  "name": "panda-manager",
  "version": "1.0.0",
  "description": "panda-manager is a cross platform manga/doujinshi manager with tag",
  "main": "scripts/client.js",
  "scripts": {
    "clear": "rimraf ./dist/generated",
    "dev": "npm run clear && ts-node-transpile-only --project tsconfig.compile.json ./build/webpack.ts --development",
    "build": "npm run clear && ts-node-transpile-only --project tsconfig.compile.json ./build/webpack.ts --production",
    "build:analyze": "npm run clear && ts-node-transpile-only --project tsconfig.compile.json ./build/webpack.ts --production --analyze",
    "package": "rimraf ./dist && npm run build && electron-packager ./dist/generated --out=dist/package --asar",
    "start": "electron --debug=5858 ./dist/generated"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.4.0",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/fs-extra": "^9.0.6",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/lodash": "^4.14.168",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "antd": "^4.11.2",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "electron": "^11.2.1",
    "electron-packager": "^15.2.0",
    "eslint": "^7.19.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^9.1.0",
    "generate-json-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^4.5.1",
    "iconv-lite": "^0.6.2",
    "image-size": "^0.9.3",
