{
  "name": "panda-manager",
  "version": "0.1.0",
  "description": "panda-manager is a cross platform manga/doujinshi manager with tag",
  "main": "dist/main/main.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev:main": "node ./build/main/webpack.dev.js",
    "build:main": "node ./build/main/webpack.prod.js",
    "dev:render": "node ./build/render/webpack.dev.js",
    "build:render": "node ./build/render/webpack.prod.js",
    "build": "npm run clear && npm run build:main && npm run build:render",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "start": "./node_modules/.bin/electron ."
  },
  "author": "xiao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "antd": "^3.10.4",
    "fs-extra": "^7.0.0",
    "image-size": "^0.6.3",
    "jszip": "^3.1.5",
    "mobx": "^5.6.0",
    "mobx-react": "^5.3.6",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "sharp": "^0.21.0",
    "string-natural-compare": "^2.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/image-size": "0.0.29",
    "@types/jszip": "^3.1.4",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^2.0.14",
    "@types/sharp": "^0.21.0",
    "@types/shelljs": "^0.8.0",
    "@types/uuid": "^3.4.4",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^3.0.8",
    "electron-rebuild": "^1.8.2",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
