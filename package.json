{
  "name": "panda-manager",
  "version": "0.1.0",
  "description": "panda-manager is a cross platform manga/doujinshi manager with tag",
  "main": "dist/main/main.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "ts-node": "node node_modules/ts-node/dist/bin.js --project tsconfig.compile.json --transpile-only",
    "dev:main": "npm run ts-node ./build/main/webpack.dev.ts",
    "build:main": "npm run ts-node ./build/main/webpack.prod.ts",
    "dev:render": "npm run ts-node ./build/render/webpack.dev.ts",
    "build:render": "npm run ts-node ./build/render/webpack.prod.ts",
    "build": "npm run clear && npm run build:main && npm run build:render",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "start": "./node_modules/.bin/electron ."
  },
  "author": "xiao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "antd": "^3.18.2",
    "fs-extra": "^8.0.1",
    "image-size": "^0.7.4",
    "jszip": "^3.2.1",
    "keyboardjs": "^2.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-use": "^9.1.2",
    "rebound": "^0.1.0",
    "sharp": "^0.22.1",
    "string-natural-compare": "^2.0.3"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.1",
    "@types/fs-extra": "^7.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/image-size": "0.7.0",
    "@types/jszip": "^3.1.6",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^12.0.2",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/sharp": "^0.22.2",
    "@types/terser-webpack-plugin": "^1.2.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.2",
    "electron-rebuild": "^1.8.5",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-loader": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.1.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslang": "^0.1.13",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2"
  }
