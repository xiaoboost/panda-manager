{
  "name": "panda-manager",
  "version": "0.1.0",
  "description": "panda-manager is a cross platform manga/doujinshi manager with tag",
  "main": "dist/main/index.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "ts-node": "node node_modules/ts-node/dist/bin.js --project tsconfig.compile.json --transpile-only",
    "dev:main": "npm run ts-node ./build/main/webpack.dev.ts",
    "build:main": "npm run ts-node ./build/main/webpack.prod.ts",
    "dev:renderer": "npm run ts-node ./build/renderer/webpack.dev.ts",
    "build:renderer": "npm run ts-node ./build/renderer/webpack.prod.ts",
    "build": "npm run clear && npm run build:main && npm run build:renderer",
    "rebuild": "./node_modules/.bin/electron-rebuild && node node_modules/sharp/install/dll-copy",
    "start": "./node_modules/.bin/electron ."
  },
  "author": "xiao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "antd": "^3.23.2",
    "fs-extra": "^8.1.0",
    "image-size": "^0.7.4",
    "jszip": "^3.2.2",
    "keyboardjs": "^2.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-event-components": "^1.1.0",
    "react-router-dom": "^5.0.1",
    "react-use": "^12.2.0",
    "sharp": "^0.23.0",
    "string-natural-compare": "^2.0.3"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/fs-extra": "^8.0.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/image-size": "0.7.0",
    "@types/jszip": "^3.1.6",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.7.5",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/sharp": "^0.22.2",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "electron": "~5.0.10",
    "electron-rebuild": "^1.8.6",
    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-loader": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.0.1",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2"
  }
}
