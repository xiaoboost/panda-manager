{
  "name": "panda-manager",
  "version": "0.1.0",
  "description": "panda-manager is a cross platform manga/doujinshi manager with tag",
  "main": "dist/main/main.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev:main": "node ./build/main/webpack.dev.js",
    "build:main": "node ./build/main/webpack.prod.js",
    "dev:render": "node ./build/render/webpack.dev.js",
    "build:render": "node ./build/render/webpack.prod.js",
    "build": "npm run clear && npm run build:main && npm run build:render",
    "start": "./node_modules/.bin/electron ."
  },
  "author": "xiao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
