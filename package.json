{
  "name": "panda-manager",
  "version": "0.1.0",
  "description": "panda-manager is a cross platform manga/doujinshi manager with tag",
  "main": "dist/main/main.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev:main": "node ./node_modules/typescript/lib/tsc.js -p ./src/tsconfig.main.json -w",
    "build:main": "cross-env NODE_ENV=production node ./node_modules/typescript/lib/tsc.js -p ./src/tsconfig.main.json --sourceMap false",
    "build:render": "cross-env NODE_ENV=production node src/render/build/webpack.prod.js",
    "build": "npm run clear && npm run build:main && npm run build:render",
    "start": "./node_modules/.bin/electron ."
  },
  "author": "xiao",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "@types/node": "^10.7.0",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/shelljs": "^0.8.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^3.0.0-beta.5",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.5.2",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "shelljs": "^0.8.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5"
  }
}
