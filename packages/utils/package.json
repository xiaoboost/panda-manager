{
  "name": "@panda/utils",
  "version": "1.0.0",
  "description": "panda-manager utils",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "tests/*.spec.ts"
    ]
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "typescript": "^4.4.2",
    "tslib": "^2.3.1"
  }
}
