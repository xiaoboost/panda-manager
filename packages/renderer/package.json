{
  "name": "@panda/renderer",
  "version": "1.0.0",
  "description": "panda-manager renderer",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "webpack --config webpack.config.js --mode development -w",
    "analyzer": "webpack-bundle-analyzer ./dist/stats.json",
    "build:data": "webpack --config webpack.config.js --mode production --json ./dist/stats.json",
    "build:analyzer": "pnpm run clear && pnpm run build:data && pnpm run analyzer",
    "build": "pnpm run clear && webpack --config webpack.config.js --mode production"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@panda/shared": "workspace:*",
    "@panda/utils": "workspace:*",
    "antd": "^4.12.1",
    "electron": "^11.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "typescript-plugin-css-modules": "^3.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.20.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  }
}
