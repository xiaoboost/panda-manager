{
  "name": "@panda/zip",
  "version": "1.0.0",
  "description": "panda-manager zip",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "tsc -w -p ./tsconfig.dev.json",
    "build:cjs": "tsc -p ./tsconfig.build.json --outDir ./dist/cjs -m CommonJS",
    "build:esm": "tsc -p ./tsconfig.build.json --outDir ./dist/esm -m ESNext",
    "build": "pnpm run clear && pnpm run build:cjs && pnpm run build:esm",
    "eslint": "eslint --fix --quiet --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "prepare": "pnpm run build"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "@panda/fs": "workspace:*",
    "@panda/utils": "workspace:*",
    "iconv-lite": "^0.6.2",
    "jszip": "^3.5.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
