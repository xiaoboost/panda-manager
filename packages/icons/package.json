{
  "name": "@panda/icons",
  "version": "1.0.0",
  "description": "panda-manager icons",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "tsc -w -p ./tsconfig.dev.json",
    "build:cjs": "tsc -p ./tsconfig.build.json --outDir ./dist/cjs -m CommonJS",
    "build:esm": "tsc -p ./tsconfig.build.json --outDir ./dist/esm -m ESNext",
    "build": "pnpm run clear && pnpm run build:cjs && pnpm run build:esm",
    "eslint": "eslint --fix --quiet --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "prepare": "pnpm run build"
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  }
}
