{
  "name": "@panda/remote",
  "version": "1.0.0",
  "description": "panda-manager renderer remote window",
  "exports": {
    "./*": {
      "import": "./src/*/index.ts",
      "types": "./src/*/index.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "@panda/shared": "workspace:*",
    "@xiao-ai/utils": "^1.5.1",
    "electron": "17.1.0"
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "@types/node": "^17.0.18",
    "@types/panda": "workspace:*",
    "tslib": "^2.3.1",
    "typescript": "4.6.2"
  }
}
