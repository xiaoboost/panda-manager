{
  "name": "@panda/db",
  "version": "1.0.0",
  "description": "panda-manager database",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "tests/*.spec.ts"
    ]
  },
  "dependencies": {
    "@xiao-ai/utils": "^1.5.1"
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "@types/node": "^17.0.18",
    "@types/panda": "workspace:*",
    "memfs": "^3.4.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
