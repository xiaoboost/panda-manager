{
  "name": "@panda/client",
  "version": "1.0.0",
  "description": "panda-manager client main code",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "build": "node build/build.js --input=src/index.ts --outDir=dist",
    "watch": "pnpm run build -- --development",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "@panda/client-utils": "workspace:*",
    "@panda/db": "workspace:*",
    "@panda/fs": "workspace:*",
    "@panda/server": "workspace:*",
    "@panda/shared": "workspace:*",
    "@panda/window-manager": "workspace:*",
    "@xiao-ai/utils": "^1.5.1",
    "electron": "^17.0.0",
    "esbuild": "^0.14.21"
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "@types/panda": "workspace:*",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
