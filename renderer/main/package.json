{
  "name": "@panda/renderer",
  "version": "1.0.0",
  "description": "panda-manager renderer",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "bundle:html": "cpy ./src/index.html ./dist",
    "bundle:dev": "esbuild src/init/index.ts --bundle --watch --outfile=./dist/index.js --format=iife --define:process.env.NODE_ENV=\\\"development\\\" --main-fields=module,main --platform=node --target=es6 --sourcemap",
    "bundle:build": "esbuild src/init/index.ts --bundle --outfile=./dist/index.js --format=iife --define:process.env.NODE_ENV=\\\"production\\\" --main-fields=module,main --platform=node --target=es6 --minify",
    "dev": "pnpm run clear && pnpm run bundle:html && pnpm run bundle:dev",
    "build": "pnpm run clear && pnpm run bundle:html && pnpm run bundle:build",
    "lint": "eslint --quiet --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "prepare": "pnpm run build"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@panda/icons": "workspace:*",
    "@panda/shared": "workspace:*",
    "@panda/use": "workspace:*",
    "@panda/utils": "workspace:*",
    "antd": "^4.13.0",
    "electron": "^11.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "@types/node": "^14.14.31",
    "@types/panda": "workspace:*",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "cpy-cli": "^3.1.1",
    "esbuild": "^0.8.54",
    "esbuild-plugin-less": "^1.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.2"
  }
}
