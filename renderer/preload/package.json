{
  "name": "@panda/preload",
  "version": "1.0.0",
  "description": "panda-manager renderer common preload script.",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "tsc -w -p ./tsconfig.dev.json",
    "build:esm": "tsc -p ./tsconfig.build.json",
    "build": "pnpm run clear && pnpm run build:esm",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "dependencies": {
    "@types/node": "^14.14.35",
    "electron": "^12.0.1"
  },
  "devDependencies": {
    "@panda/tsconfig": "workspace:*",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
